{
  "multipart": [
    {
      "apply": {"model": "autotech:block/pipe"}
    }, {
      "when": {
        "north": true
      },
      "apply": {"model": "autotech:block/pipe_multipart"}
    },
    {
      "when": {
        "east": true
      },
      "apply": {"model": "autotech:block/pipe_multipart", "y": 90}
    },
    {
      "when": {
        "south": true
      },
      "apply": {"model": "autotech:block/pipe_multipart", "x": 180}
    },
    {
      "when": {
        "west": true
      },
      "apply": {"model": "autotech:block/pipe_multipart", "y": 270}
    },
    {
      "when": {
        "up": true
      },
      "apply": {"model": "autotech:block/pipe_multipart", "x": 270}
    },
    {
      "when": {
        "down": true
      },
      "apply": {"model": "autotech:block/pipe_multipart", "x": 90}
    }
  ]
}